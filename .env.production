# Production Environment Variables for Azure VM
# Copy this file to .env and fill in the actual values

# Azure OpenAI Configuration
AZURE_OPENAI_ENDPOINT=https://factagora-aoai-eastus.openai.azure.com/
AZURE_OPENAI_API_KEY=your_azure_openai_api_key_here
AZURE_OPENAI_DEPLOYMENT=gpt-4o

# Neo4j Configuration (container internal)
NEO4J_URI=bolt://neo4j:7687
NEO4J_USER=neo4j
NEO4J_PASSWORD=password

# Optional: For external access
# OPENAI_API_KEY=your_fallback_openai_key_here